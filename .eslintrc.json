{
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": "off",
        "no-useless-escape": "off",
        "no-unused-vars": "off",
        "eol-last": [
            "error",
            "always"
        ],
        "no-undef": "off"
    }
}
