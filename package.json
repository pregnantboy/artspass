    "cross-env": "^5.1.3",
    "eslint": "^4.16.0",
    "eslint-config-google": "^0.9.1",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^15.2.0",
    "browserify-hmr": "^0.3.6",
