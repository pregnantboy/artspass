{
  "name": "Vault",
  "version": "2.0.0",
  "manifest_version": 2,
  "description": "All your password are belong to us.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "options_ui": {
    "page": "src/options/options.html",
    "chrome_style": true,
    "open_in_tab": false
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "ARTSpass",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "oauth2": {
    "client_id": "542991076028-sdu76a4klhdjk4og7c99pk9qslnff48j.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "permissions": [
    "clipboardWrite",
    "storage",
    "identity",
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com  https://apis.google.com; object-src 'self'"
}
